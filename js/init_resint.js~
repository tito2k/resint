function terminarSesion()
{
	$('#logoff').load('prcTerminarSesion.php',{idSesion:$('#idSesion').val()})
	window.location = 'http://localhost/resint/index.html';
};

function ejecutarOpcion()
{
	var pnt = this.getAttribute('proc');
	$('#center').load(pnt);
};